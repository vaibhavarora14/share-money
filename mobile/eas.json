{
  "cli": {
    "version": ">= 5.2.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "EAS_BUILD": "true",
        "EXPO_PUBLIC_USE_DEV_CLIENT": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "EXPO_NO_UPDATES": "1",
        "EAS_BUILD": "true",
        "EXPO_PUBLIC_USE_DEV_CLIENT": "true"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "EAS_BUILD": "true"
      },
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}